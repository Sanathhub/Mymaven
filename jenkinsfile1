pipeline
{  agent any
   stages()
   {  
      stage('ContDownload')
      {  
         steps()
         {
           git 'https://github.com/Sanathhub/Mymaven.git'
         }
      }
      stage('ContBuild')
      {  
         steps()
         {
           sh 'mvn package'
         }
      }
   }
}
 
